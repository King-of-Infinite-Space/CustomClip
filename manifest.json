{
  "name": "Web Fetcher",
  "manifest_version": 3,
  "version": "0.3.0",
  "description": "Prototype",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "src/rules.js",
        "src/content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "src/settings.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "src/popup.html"
  },
  "icons": {
    "256": "icon256.png"
  }
}