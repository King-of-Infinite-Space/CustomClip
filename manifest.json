{
  "name": "Web Fetcher",
  "manifest_version": 3,
  "version": "0.2",
  "description": "Prototype",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "extractor/utils.js"
      ]
    },
    {
      "matches": [
        "https://book.douban.com/subject/*"
      ],
      "js": [
        "extractor/doubanBook.js"
      ]
    },
    {
      "matches": [
        "https://store.steampowered.com/app/*"
      ],
      "js": [
        "extractor/steam.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "web-fetcher-dev@example.com"
    }
  }
}